<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Panes.EditController">
    <children>
        <TextField fx:id="wordToEdited" layoutX="58.0" layoutY="99.0" promptText="Word to edit..." />
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="205.0" layoutY="44.0" prefHeight="43.0" prefWidth="190.0" text="EDIT WORD" />
        <Button fx:id="editBtn" layoutX="132.0" layoutY="265.0" mnemonicParsing="false" onAction="#editWord" prefHeight="35.0" prefWidth="76.0"  stylesheets="@/css/style.css" text="Edit" />
        <Button layoutX="373.0" layoutY="265.0" mnemonicParsing="false" onAction="#back" prefHeight="35.0" prefWidth="76.0" stylesheets="@/css/style.css" text="Back" />
      <TextField fx:id="wordEdited" layoutX="373.0" layoutY="99.0" prefHeight="26.0" prefWidth="169.0" promptText="Edited to..." />
      <TextField fx:id="explain" layoutX="373.0" layoutY="166.0" prefHeight="26.0" prefWidth="170.0" promptText="Explaination" />
    </children>
</AnchorPane>
